{
  "name": "waka-next",
  "version": "0.0.1",
  "homepage": "https://github.com/consindo/waka-next",
  "license": "MIT",
  "scripts": {
    "dev": "npm run dev:orchestrator & npm run dev:web",
    "dev:orchestrator": "cd apps/orchestrator && vite dev",
    "dev:web": "cd apps/web && vite dev",
    "build": "npm run build:orchestrator && npm run build:web",
    "build:orchestrator": "cd apps/orchestrator && vite build",
    "build:web": "cd apps/web && vite build && cp -r .netlify ../../",
    "preview": "npm run preview:orchestrator & npm run preview:web",
    "preview:orchestrator": "cd apps/orchestrator && vite preview",
    "preview:web": "cd apps/web && vite preview",
    "check": "npm run check:orchestrator && npm run check:web",
    "check:orchestrator": "cd apps/orchestrator && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:web": "cd apps/web && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "test": "vitest",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.7.10",
    "@sveltejs/adapter-netlify": "^5.0.0",
    "@sveltejs/kit": "^2.20.7",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/eslint": "^9.6.1",
    "@types/sql.js": "^1.4.9",
    "@vitest/coverage-v8": "^3.1.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-svelte": "^3.5.1",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "rollup-plugin-visualizer": "^5.14.0",
    "svelte": "^5.27.3",
    "svelte-check": "^4.1.6",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0",
    "vite": "^6.3.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.1"
  },
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.787.0",
    "@date-fns/tz": "^1.2.0",
    "@gera2ld/tarjs": "^0.3.1",
    "@loaders.gl/core": "^4.3.3",
    "@loaders.gl/wkt": "^4.3.3",
    "@sveltejs/adapter-node": "^5.2.12",
    "@turf/bbox": "^7.2.0",
    "@types/geojson": "^7946.0.16",
    "@zip.js/zip.js": "^2.7.60",
    "confbox": "^0.2.2",
    "date-fns": "^4.1.0",
    "maplibre-gl": "^5.6.0",
    "sql.js": "^1.13.0",
    "udsv": "^0.6.0"
  }
}
