{
  "name": "waka-next",
  "version": "0.0.1",
  "homepage": "https://github.com/consindo/waka-next",
  "license": "MIT",
  "scripts": {
    "dev": "npm run dev:orchestrator & npm run dev:web",
    "dev:orchestrator": "cd apps/orchestrator && vite dev",
    "dev:web": "cd apps/web && vite dev",
    "build": "npm run build:orchestrator && npm run build:web",
    "build:orchestrator": "cd apps/orchestrator && vite build",
    "build:web": "cd apps/web && vite build",
    "preview": "npm run preview:orchestrator & npm run preview:web",
    "preview:orchestrator": "cd apps/orchestrator && vite preview",
    "preview:web": "cd apps/web && vite preview",
    "check": "npm run check:orchestrator && npm run check:web",
    "check:orchestrator": "cd apps/orchestrator && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:web": "cd apps/web && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "test": "vitest",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.5.5",
    "@sveltejs/adapter-netlify": "^4.2.0",
    "@sveltejs/kit": "^2.5.6",
    "@sveltejs/vite-plugin-svelte": "^3.1.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/eslint": "^8.56.9",
    "@types/sql.js": "^1.4.9",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "@vitest/coverage-v8": "^1.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.37.0",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.3",
    "rollup-plugin-visualizer": "^5.12.0",
    "svelte": "^4.2.15",
    "svelte-check": "^3.6.9",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.9",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.5.0"
  },
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.556.0",
    "@gera2ld/tarjs": "^0.3.1",
    "@loaders.gl/core": "^4.2.0",
    "@loaders.gl/wkt": "^4.2.0",
    "@sveltejs/adapter-node": "^5.0.1",
    "@types/geojson": "^7946.0.14",
    "@zip.js/zip.js": "^2.7.41",
    "sql.js": "^1.10.3",
    "udsv": "^0.5.3",
    "yaml": "^2.4.1"
  }
}
