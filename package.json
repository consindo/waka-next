{
  "name": "waka-next",
  "version": "0.0.1",
  "homepage": "https://github.com/consindo/waka-next",
  "license": "MIT",
  "scripts": {
    "dev": "npm run dev:orchestrator & npm run dev:web",
    "dev:orchestrator": "cd apps/orchestrator && vite dev",
    "dev:web": "cd apps/web && vite dev",
    "build": "npm run build:orchestrator && npm run build:web",
    "build:orchestrator": "cd apps/orchestrator && vite build",
    "build:web": "cd apps/web && vite build",
    "build:netlify": "npm run build:web && ln -s ./apps/web/.netlify ./.netlify",
    "preview": "npm run preview:orchestrator & npm run preview:web",
    "preview:orchestrator": "cd apps/orchestrator && vite preview",
    "preview:web": "cd apps/web && vite preview",
    "check": "npm run check:orchestrator && npm run check:web",
    "check:orchestrator": "cd apps/orchestrator && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:web": "cd apps/web && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "test": "vitest",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-netlify": "^4.1.0",
    "@sveltejs/kit": "^2.5.4",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@types/eslint": "^8.56.5",
    "@types/sql.js": "^1.4.9",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitest/coverage-v8": "^1.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "svelte": "^4.2.12",
    "svelte-check": "^3.6.7",
    "tslib": "^2.6.2",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
    "vitest": "^1.3.1"
  },
  "type": "module",
  "dependencies": {
    "@sveltejs/adapter-node": "^5.0.1",
    "@zip.js/zip.js": "^2.7.40",
    "sql.js": "^1.10.2",
    "udsv": "^0.5.3"
  }
}
